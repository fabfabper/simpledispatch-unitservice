{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Api": {
    "HttpPort": 5001,
    "EnableHttpsRedirection": false,
    "EnableSwaggerInProduction": false
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=simpledispatch;Username=postgres;Password=password"
  },
  "Database": {
    "MaxRetryCount": 3,
    "MaxRetryDelay": 30,
    "EnableSensitiveDataLogging": false,
    "EnableDetailedErrors": false,
    "CommandTimeout": 30
  },
  "RabbitMq": {
    "HostName": "localhost",
    "Port": 5672,
    "UserName": "simpledispatch_user",
    "Password": "simpledispatch_password",
    "VirtualHost": "/",
    "QueueName": "units",
    "ExchangeName": "simpledispatch.exchange",
    "ExchangeType": "direct",
    "Durable": true,
    "AutoAck": false,
    "PrefetchCount": 1
  },
  "RabbitMqProducer": {
    "HostName": "localhost",
    "Port": 5672,
    "UserName": "simpledispatch_user",
    "Password": "simpledispatch_password",
    "VirtualHost": "/",
    "QueueName": "units",
    "ExchangeName": "notifications",
    "ExchangeType": "direct",
    "Durable": true,
    "AutoAck": false,
    "PrefetchCount": 1
  }
}
