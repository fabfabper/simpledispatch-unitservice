{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Api": {
    "HttpPort": 5002,
    "EnableHttpsRedirection": false,
    "EnableSwaggerInProduction": true
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost:5432;Database=simpledispatch;Username=simpledispatch_user;Password=simpledispatch_password"
  },
  "Database": {
    "MaxRetryCount": 3,
    "MaxRetryDelay": 30,
    "EnableSensitiveDataLogging": true,
    "EnableDetailedErrors": true,
    "CommandTimeout": 30
  },
  "RabbitMq": {
    "HostName": "localhost",
    "Port": 5672,
    "UserName": "simpledispatch_user",
    "Password": "simpledispatch_password",
    "VirtualHost": "/",
    "QueueName": "units",
    "ExchangeName": "simpledispatch.exchange",
    "ExchangeType": "direct",
    "Durable": true,
    "AutoAck": false,
    "PrefetchCount": 1
  },
  "RabbitMqProducer": {
    "HostName": "localhost",
    "Port": 5672,
    "UserName": "simpledispatch_user",
    "Password": "simpledispatch_password",
    "VirtualHost": "/",
    "QueueName": "notifications",
    "ExchangeName": "notifications.exchange",
    "ExchangeType": "topic",
    "Durable": true,
    "AutoAck": false,
    "PrefetchCount": 1
  }
}
